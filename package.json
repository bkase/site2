{
  "name": "with-reasonml",
  "version": "1.0.0",
  "scripts": {
    "dev": "concurrently 'yarn dev:reason' 'yarn dev:next'",
    "dev:reason": "bsb -clean-world -make-world -w",
    "dev:next": "next dev -p 8000",
    "clean": "bsb -clean-world",
    "build": "bsb -make-world && yarn gen-rss && next build",
    "export": "yarn build && next export",
    "start": "next start -p 8000",
    "gen-rss": "node -r esm gen-rss.js"
  },
  "license": "ISC",
  "dependencies": {
    "@babel/core": "^7.8.0",
    "@babel/plugin-transform-modules-commonjs": "^7.8.0",
    "@babel/preset-react": "^7.8.0",
    "@mdx-js/loader": "^1.5.1",
    "@mdx-js/react": "^1.5.1",
    "@next/mdx": "^9.1.5",
    "babel-plugin-module-resolver": "^4.0.0",
    "babel-preset-env": "^1.7.0",
    "bs-css": "^9.0.1",
    "bsc-stdlib-polyfill": "Schmavery/bsc-stdlib-polyfill",
    "contentful": "^7.10.0",
    "emotion-server": "^10.0.17",
    "esm": "^3.2.25",
    "isomorphic-unfetch": "^3.0.0",
    "next": "latest",
    "next-transpile-modules": "2.3.1",
    "nextjs-morph-page": "^2.0.3",
    "prism-react-renderer": "^1.0.2",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "reason-react": "^0.7.0",
    "rehype-katex": "^2.0.0",
    "rehype-raw": "^4.0.1",
    "rehype-react": "^4.0.1",
    "rehype-sanitize": "^3.0.0",
    "remark-math": "^2.0.0",
    "remark-parse": "^7.0.2",
    "remark-rehype": "^5.0.0",
    "unified": "^8.4.2"
  },
  "devDependencies": {
    "@next/bundle-analyzer": "^9.1.4",
    "bs-platform": "^7.0.1",
    "concurrently": "^4.1.0",
    "serve": "^11.1.0"
  }
